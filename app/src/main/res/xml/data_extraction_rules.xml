<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude cache and temporary files from cloud backup -->
        <exclude domain="cache" />
        <exclude domain="no_backup" />
    </cloud-backup>
    <device-transfer>
        <!-- Include app data for device transfer -->
        <include domain="file" />
        <include domain="database" />
    </device-transfer>
</data-extraction-rules>